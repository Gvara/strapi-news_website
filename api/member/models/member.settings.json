{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "members",
  "info": {
    "name": "Member"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Description": {
      "type": "text"
    },
    "Alias": {
      "type": "uid",
      "targetField": "Title"
    },
    "Content": {
      "type": "richtext"
    },
    "Image": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "ImageTitle": {
      "type": "string"
    },
    "ImageDescription": {
      "type": "string"
    },
    "MenuIndex": {
      "type": "integer",
      "default": 9999
    }
  }
}
