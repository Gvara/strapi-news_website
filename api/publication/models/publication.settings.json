{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "publications",
  "info": {
    "name": "Publication"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "Title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "Description": {
      "type": "text",
      "required": true
    },
    "Content": {
      "type": "richtext",
      "required": true
    },
    "Poster_Main": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": true
    },
    "author": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "publications"
    }
  }
}
